{
    "kind": "collectionType",
    "collectionName": "blog_categories",
    "info": {
        "singularName": "blog-category",
        "pluralName": "blog-categories",
        "displayName": "blog-category"
    },
    "options": {
        "draftAndPublish": true
    },
    "attributes": {
        "categoryName": {
            "type": "string"
        },
        "description": {
            "type": "text"
        },
        "blog": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::blog.blog",
            "mappedBy": "categoryName"
        }
    }
}
