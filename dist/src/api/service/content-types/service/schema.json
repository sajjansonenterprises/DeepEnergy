        },
        "heading": {
            "type": "string"
        },
        "Advantages": {
            "type": "component",
            "repeatable": false,
            "component": "pagesetting.advantage"
        },
        "iconFontAwesome": {
            "type": "string"
        },
        "metaTags": {
            "type": "string"
        },
        "metaDescription": {
            "type": "text"
        },
        "content": {
            "type": "richtext"
        }
